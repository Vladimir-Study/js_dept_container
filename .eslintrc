{
    "env": {
        "browser": true,
        "es2022": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-sequences": ["error", {"allowInParentheses": false}]
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    }
}